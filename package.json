{
  "name": "vite-reactts-eslint-prettier",
  "version": "0.0.0",
  "scripts": {
    "dev": "ts-node server.ts",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "build": "npm run build:client && npm run build:server",
    "preview": " cross-env NODE_ENV=production ts-node server.ts",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cy:run": "cypress run --browser chrome && nyc report --reporter=text",
    "cy:e2e": "start-server-and-test dev http-get://localhost:5173 cy:run"
  },
  "type": "module",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@types/jest": "^29.5.0",
    "@types/parse": "^3.0.4",
    "@types/react-redux": "^7.1.25",
    "axios": "^1.3.4",
    "body-scroll-lock": "^4.0.0-beta.0",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "parse": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "sirv": "^2.0.2",
    "sort-by": "^0.0.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/compression": "^1.7.2",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/events": "^3.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.3",
    "@types/node-fetch": "^2.6.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "@vitest/ui": "^0.29.3",
    "compression": "~1.7.4",
    "cross-env": "^7.0.3",
    "cypress": "^12.11.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.4",
    "react-hook-form": "^7.43.9",
    "start-server-and-test": "^2.0.0",
    "typescript": "^4.9.5",
    "typescript-plugin-css-modules": "^4.2.3",
    "vite": "^4.1.4",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.3"
  },
  "pre-commit": "lint",
  "license": "MIT",
  "msw": {
    "workerDirectory": "public"
  }
}
